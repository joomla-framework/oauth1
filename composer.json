{
    "name": "joomla/oauth1",
    "type": "joomla-package",
    "description": "Joomla OAuth1 Package",
    "keywords": ["joomla", "framework", "oauth1"],
    "homepage": "https://github.com/joomla-framework/oauth1",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "~8.1.0",
        "joomla/application": "3.0.x-dev",
        "joomla/http": "3.0.x-dev",
        "joomla/input": "3.0.x-dev",
        "joomla/registry": "3.0.x-dev",
        "joomla/session": "3.0.x-dev",
        "joomla/uri": "3.0.x-dev"
    },
    "require-dev": {
        "joomla/event": "3.0.x-dev",
        "joomla/test": "3.0.x-dev",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "~3.7.1"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\OAuth1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\OAuth1\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev",
            "dev-3.x-dev": "3.0-dev"
        }
    }
}
